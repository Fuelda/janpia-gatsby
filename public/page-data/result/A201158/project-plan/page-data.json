nd_distr":"F200116","biz_cd_executive":"A201158","row_no":4,"info_type":"33","business_goals":{"data":{"childMarkdownRemark":{"html":""}}},"goals_monitoring":{"data":{"childMarkdownRemark":{"html":""}}},"goals_index":{"data":{"childMarkdownRemark":{"html":""}}},"goals_index_arrival":{"data":{"childMarkdownRemark":{"html":""}}},"goals_grasp":{"data":{"childMarkdownRemark":{"html":""}}},"goals_initial":{"data":{"childMarkdownRemark":{"html":""}}},"goals_goal":{"data":{"childMarkdownRemark":{"html":""}}},"goals_achievement":{"data":{"childMarkdownRemark":{"html":""}}},"goals_mid_eval":{"data":{"childMarkdownRemark":{"html":""}}},"goals_aft_eval":{"data":{"childMarkdownRemark":{"html":""}}},"output":{"data":{"childMarkdownRemark":{"html":""}}},"output_monitor":{"data":{"childMarkdownRemark":{"html":""}}},"output_index":{"data":{"childMarkdownRemark":{"html":""}}},"output_index_arrival":{"data":{"childMarkdownRemark":{"html":""}}},"output_grasp":{"data":{"childMarkdownRemark":{"html":""}}},"output_initial":{"data":{"childMarkdownRemark":{"html":""}}},"output_goal":{"data":{"childMarkdownRemark":{"html":""}}},"output_achievement":{"data":{"childMarkdownRemark":{"html":""}}},"output_activity":{"data":{"childMarkdownRemark":{"html":""}}},"output_season":{"data":{"childMarkdownRemark":{"html":""}}},"output_mid_eval":{"data":{"childMarkdownRemark":{"html":""}}},"output_aft_eval":{"data":{"childMarkdownRemark":{"html":""}}},"activity":{"data":{"childMarkdownRemark":{"html":"<p>修了者へのフォローについて\n女性がリーダーになっていくためには、学び・気づき・考え・行動するというプロセスが重要であり、そのプロセスによって自信につながり、エンパワーメントしていく。（下記、フローチャート参照）　その考え方から、修了生へのフォローにおいては、「仲間づくり」と「実践の場」の提供に力を入れる。</p>\n<p>【フォローアップ講座の開催】\n目的：ふりかえりや自主防災組織との意見交換を通して、モチベーションアップと講座修了後の行動変容につなげる\n・ステップアップ講座の開催\n・防災カフェの開催（活動仲間のネットワークづくり及び地域住民との交流）\n【フォローアップ支援事業】\n・共催事業実施希望団体に対して：実践活動のバックアップ（2団体）\n・当社団が継続して実施している「若年層へのジェンダー視点を取り入れた避難所運営体験」へのサポーターとして参加することで実践を積み重ねる機会の提供。（希望者）\n【＊２サイクル終了後、合同研修】\n・防災カフェとして開催：2025年2月\n修了生の同窓会の実施、２地域合同で活動仲間のネットワークづくりにつなげる\nワークショップ、事例報告などを実施\n【報告書の作成】\n・実施内容や成果をまとめて報告書を作成し、事例集として活用していく。</p>"}}},"activity_season":{"data":{"childMarkdownRemark":{"html":"<p>－</p>"}}},"sdgs_goal":null,"sdgs_target":null,"sdgs_description":{"data":{"childMarkdownRemark":{"html":""}}},"create_date":"2023/00/Th","insert_id":"8gsfhrfz8q2v9xe"}},{"node":{"updatedAt":"2023-10-03T02:21:51.964Z","business_cd":"A201158","business_org_type":"A","biz_cd_fund_distr":"F200116","biz_cd_executive":"A201158","row_no":5,"info_type":"33","business_goals":{"data":{"childMarkdownRemark":{"html":""}}},"goals_monitoring":{"data":{"childMarkdownRemark":{"html":""}}},"goals_index":{"data":{"childMarkdownRemark":{"html":""}}},"goals_index_arrival":{"data":{"childMarkdownRemark":{"html":""}}},"goals_grasp":{"data":{"childMarkdownRemark":{"html":""}}},"goals_initial":{"data":{"childMarkdownRemark":{"html":""}}},"goals_goal":{"data":{"childMarkdownRemark":{"html":""}}},"goals_achievement":{"data":{"childMarkdownRemark":{"html":""}}},"goals_mid_eval":{"data":{"childMarkdownRemark":{"html":""}}},"goals_aft_eval":{"data":{"childMarkdownRemark":{"html":""}}},"output":{"data":{"childMarkdownRemark":{"html":""}}},"output_monitor":{"data":{"childMarkdownRemark":{"html":""}}},"output_index":{"data":{"childMarkdownRemark":{"html":""}}},"output_index_arrival":{"data":{"childMarkdownRemark":{"html":""}}},"output_grasp":{"data":{"childMarkdownRemark":{"html":""}}},"output_initial":{"data":{"childMarkdownRemark":{"html":""}}},"output_goal":{"data":{"childMarkdownRemark":{"html":""}}},"output_achievement":{"data":{"childMarkdownRemark":{"html":""}}},"output_activity":{"data":{"childMarkdownRemark":{"html":""}}},"output_season":{"data":{"childMarkdownRemark":{"html":""}}},"output_mid_eval":{"data":{"childMarkdownRemark":{"html":""}}},"output_aft_eval":{"data":{"childMarkdownRemark":{"html":""}}},"activity":{"data":{"childMarkdownRemark":{"html":"<p>地域への理解促進・連携等について\n【テレビ番組の制作】\n目的\n　・県民に対し、防災・減災をはじめ、地域リーダーへの女性の参画の必要性の理解と啓発\n・特に男性の理解につなげる\n・本事業の県民への周知活動につなげる\n　・2サイクル目の受講生の募集につなげる\nテーマ「女性の参画で地域防災力を高める」（仮称）\n出演者\n・1サイクルの受講生（2人）\n・行政（首長・防災関係者）\n・地域防災に関わっている方（地域の方）\n＊約30分番組を制作し、地上波のテレビ局で放送する。\n＊なお、番組制作にあたっては、版権を当社団が持つことにより、県内にあるケーブルテレビにも放送を依頼し、さらなる地域の理解促進につなげる。</p>\n<p>【連携】\n○青森県、青森市など開催市町村に後援依頼をし、信頼性を高めて事業を進めていく。\nまた、青森県危機管理局防災危機管理課等に協力依頼をすることで、円滑な育成プログラムの推進と女性が地域でリーダーとして活躍できる基盤整備につなげる。\n○「女性のエンパワーで高まる防災・減災連携会議」の開催\n目的：地域の様々な立場の方に集まっていただき、本事業への助言及び周知と理解、協力を得る。また、受講生の修了後のフォローアップにもつなげる。\n委員：行政（防災担当課、男女共同参画担当課）、町内会・自主防災組織、まちづくり協議会、社会福祉協議会、大学、青森県防災士会、障がい者団体　など\n開催回数：3回（実施前、実施途中、実施後）</p>"}}},"activity_season":{"data":{"childMarkdownRemark":{"html":"<p>－</p>"}}},"sdgs_goal":null,"sdgs_target":null,"sdgs_description":{"data":{"childMarkdownRemark":{"html":""}}},"create_date":"2023/00/Th","insert_id":"8gsfhrfz8q2v9xe"}}]}},"pageContext":{"slug":"A201158"}},"staticQueryHashes":["1595297291","1725940062"],"slicesMap":{}}